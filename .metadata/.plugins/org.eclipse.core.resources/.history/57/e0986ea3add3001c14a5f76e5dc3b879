package pat;

public class PrimitiveDatatypes {
	
	public static void main(String[] args) {
		
		byte a = 0;
		short b;
		long c;
		int d;
		float e;
		double f;
		char g;
		
		System.out.println("byte a: " + a + 
						   "\nshort b: " + b + 
						   "\nlong c: " + c + 
						   "\nint d: " + d +
						   "\nfloat e: " + e +
						   "\ndouble f :" + f +
						   "\nchar g: " + g);

	}

}
